<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main_navigation"
    app:startDestination="@id/welcomeFragment">

    <fragment
        android:id="@+id/welcomeFragment"
        android:name="com.konovalov.compositiongame.presentation.WelcomeFragment"
        android:label="WelcomeFragment" >
        <action
            android:id="@+id/action_welcomeFragment_to_chooseMathModeFragment2"
            app:destination="@id/chooseMathModeFragment2" />
    </fragment>
    <fragment
        android:id="@+id/chooseMathModeFragment2"
        android:name="com.konovalov.compositiongame.presentation.ChooseMathModeFragment"
        android:label="ChooseMathModeFragment" >
        <action
            android:id="@+id/action_chooseMathModeFragment2_to_chooseLevelFragment2"
            app:destination="@id/chooseLevelFragment2" />
    </fragment>
    <fragment
        android:id="@+id/chooseLevelFragment2"
        android:name="com.konovalov.compositiongame.presentation.ChooseLevelFragment"
        android:label="ChooseLevelFragment" >
        <action
            android:id="@+id/action_chooseLevelFragment2_to_gameFragment2"
            app:destination="@id/gameFragment2" />
    </fragment>
    <fragment
        android:id="@+id/gameFragment2"
        android:name="com.konovalov.compositiongame.presentation.GameFragment"
        android:label="fragment_game"
        tools:layout="@layout/fragment_game" >
        <action
            android:id="@+id/action_gameFragment2_to_gameFinishedFragment2"
            app:destination="@id/gameFinishedFragment2"
            app:popUpTo="@id/gameFragment2"
            app:popUpToInclusive="true" />
    </fragment>
    <fragment
        android:id="@+id/gameFinishedFragment2"
        android:name="com.konovalov.compositiongame.presentation.GameFinishedFragment"
        android:label="fragment_game_finished"
        tools:layout="@layout/fragment_game_finished" >
        <action
            android:id="@+id/action_gameFinishedFragment2_to_chooseMathModeFragment2"
            app:destination="@id/chooseMathModeFragment2" />
    </fragment>
</navigation>